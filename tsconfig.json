{
  "compilerOptions": {
    "target": "ES2024",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "rootDir": "./",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx"
  }
}
